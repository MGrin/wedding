{
  "name": "@scani/wedding",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "predev": "bun x @tailwindcss/cli -i src/index.css -o src/compiled.css",
    "dev": "bun x @tailwindcss/cli -i src/index.css -o src/compiled.css --watch & bun --hot src/index.ts",
    "start": "NODE_ENV=production bun src/index.ts",
    "build": "bun x @tailwindcss/cli -i src/index.css -o src/compiled.css && bun run build.ts",
    "preview": "bun run build && bun run preview.ts"
  },
  "dependencies": {
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "bun-plugin-tailwind": "^0.1.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.26",
    "lucide-react": "^0.545.0",
    "react": "^19",
    "react-dom": "^19",
    "react-router-dom": "^7.11.0",
    "tailwind-merge": "^3.3.1",
    "three": "^0.182.0",
    "use-sound": "^5.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/three": "^0.182.0",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.4.0"
  }
}
